{
  "name": "jsdoc-tag-parser",
  "description": "Strict JsDoc type expression parser.",
  "version": "1.0.0",
  "author": "Julian Jensen <jjdanois@gmail.com>",
  "keywords": [
    "jsdoc",
    "type expression",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/julianjensen/jsdoc-tag-parser.git"
  },
  "directories": {
    "test": "test"
  },
  "main": "index.js",
  "scripts": {
    "prepublish": "npm-run-all build",
    "pretest1": "npm-run-all lint build",
    "test": "mocha test",
    "lint": "eslint lib test",
    "clean": "rimraf ./peg_lib",
    "postclean": "mkdirp ./peg_lib",
    "prebuild": "npm-run-all clean",
    "build": "pegjs --cache -o ./peg_lib/jsdoctype.js ./peg_src/jsdoctype.pegjs"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "pegjs": "^0.10.0",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "dependencies": {
    "doctrine": "latest",
    "espree": "latest",
    "estraverse": "latest",
    "commander": "latest"
  }
}
